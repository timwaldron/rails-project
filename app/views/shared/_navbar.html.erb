<div class="navbar">

  <div class="navbar-links">
    <%= link_to 'Home', root_path %>
    <%= link_to "My Listings", show_user_listings_path(current_user.id) if user_signed_in? %>
    <%= link_to "Sell A Game", new_game_path %>
  </div>
  <div class="navbar-links">
    <% if user_signed_in? %>
      <%= link_to 'Profile', edit_user_registration_path %>
      <%= link_to 'History', user_transactions_path %>
      <%= link_to "Log Out (#{current_user.username})", destroy_user_session_path, method: :delete %>
    <% else %>
      <%= link_to 'Log In', new_user_session_path %>
      <%= link_to 'Sign Up', new_user_registration_path %>
    <% end %>
  </div>

</div>

